    <edge source="Phone" target="Google Play Store/App Store">
      <data key="d3">9.0</data>
      <data key="d4">The Google Play Store and App Store provide a platform for downloading applications on Android and iOS devices respectively.</data>
      <data key="d5">app access, digital marketplace</data>
      <data key="d6">chunk-016a36b967af746fcdce02d11a3eca50</data>
